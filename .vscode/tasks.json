  "version": "2.0.0",
  "tasks": [
    {
      "label": "build-dev",
      "type": "shell",
      "command": "env ng build --base-href=/blog/ --watch --output-path=dist/blog-angular",
      "group": "build",
      "problemMatcher": [
          "$msCompile"
      ]
  },
  {
      "label": "build-production",
      "type": "shell",
      "command": "ng build --base-href=/ --configuration production",
      "group": "build",
      "problemMatcher": [
          "$msCompile"
      ]
  },
  ]
}
